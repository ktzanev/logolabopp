\definecolor{bleufonceuniv}{rgb}{0.086,0.467,0.721}
\definecolor{noiruniv}{rgb}{0.102,0.090,0.090}
\definecolor{grisuniv}{rgb}{0.486,0.509,0.509}

\definecolor{vertinsmi}{rgb}{0.33,0.49,0.24}
\definecolor{vertfonce}{rgb}{0.105,0.349,0.188} % rgb(27,89,48) 
\definecolor{vertclaire}{rgb}{0.25,0.839,0.447} % rgb(64,214,114)

% ------------------- default draw and fill parameters ------------------------
\def\setdefaultspp{
\def\setup{[shade, top color=vertclaire, bottom color=vertfonce]}
\def\setdown{}
\def\setborder{}
\def\setcircles{[draw=gray!70!vertclaire, thin, opacity=0.3]} % gray!70!vertclaire = rgb(108,153,124)
\def\settextlabo{[fill=black]}
\def\settextheader{[text width=7cm, gray, scale=0.84]}
}
\setdefaultspp

% -------------------  P path ------------------------
\def\ppath{(0,-2) arc (-90:-270:1) arc (-90:90:1)--++(-1.5,0)--++(0,-4)--cycle
      (0,0.5) arc (-90:90:0.5)--++(-1,0)--++(0,-1.382) arc (131.81:90:1.5)--cycle
      (0,-0.5) arc (90:270:0.5)--++(1,0)--++(0,1.382) arc (-48.19:-90:1.5)--cycle}

% ------------------- the up P ------------------------
\def\drawup{
    \begin{scope}[even odd rule]
        \path\setup \ppath ;
    \end{scope}
}

% ------------------- the down P ------------------------
\def\drawdown{
    \begin{scope}[even odd rule, rotate=180]
        \path\setdown \ppath ;
    \end{scope}
}

% ------------------- border path ------------------------
\def\pborder{(-1.5,-2) rectangle (1.5,2)}

% ------------------- the border ------------------------
\def\drawborder{
    \path\setborder \pborder;
}

% ------------------- the circles ------------------------
\def\drawcircles{
    \begin{scope}
        \path\setcircles (0,-1) circle (0.5) circle (1) circle (1.5);
        \path\setcircles (0,1) circle (0.5) circle (1) circle (1.5);
        \foreach \x in {-1.5,-1,1,1.5}
            \path\setcircles (\x,-2.5) -- (\x,2.5);
        \foreach \y in {-2,-1.5,1.5,2}
            \path\setcircles (-2,\y) -- (2,\y);
    \end{scope}
}

